#include <stdio.h>
int main() {
    int n, i, sum = 0;
    printf("Enter the number of elements in the array\n ");
    scanf("%d", &n);

    int arr[10]; 
    printf("Enter the elements of array\n");
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        sum = sum + arr[i];
    }
    printf("Sum is equal to %d\n", sum);
    return 0;
}
